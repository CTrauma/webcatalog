  <meta property="og:site_name" content="WebCatalog">
  <meta property="og:title" content="<%= locals.title ? title + ' | ' : '' %>WebCatalog">
  <meta property="og:type" content="article">
  <meta property="og:description" content="<%= description %>">
  <meta property="og:locale" content="en_US">
  <meta property="og:url" content="https://webcatalog.io<%= path %>">
  <% if (locals.app) { %><meta property="og:image" content="https://icons.webcatalog.io/<%= app.id %>/<%= app.id %>-icon.png"><% } %>
  <link rel="shortcut icon" href="/public/images/favicon.png" type="image/png">
  <link rel="apple-touch-icon" sizes="57x57" href="/public/images/apple-touch-icon-57x57.png">
